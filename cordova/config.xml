<?xml version='1.0' encoding='utf-8'?>
<widget id="com.billgauvey.iplm" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>IP List Manager</name>
    <description>
        IP List Manager
    </description>
    <author email="bgauvey@cgmail.com" href="http://billgauvey.com">
        Bill Gauvey
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="res/app-icon.png" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
</widget>
